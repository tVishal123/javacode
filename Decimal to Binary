
import java.util.*;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here

        
        
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int b[] = new int[40];
        int i = 0;
        while(n > 0)
        {
          b[i++] = n%2;
          n = n/2;
         }
        long bn=0;
         for(int j = i-1;j>=0;j--)
         {
           bn = bn*10+b[j];
         }
        System.out.print(bn);
	}
}
